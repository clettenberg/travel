<h1>
  Edit
  <%= link_to 'Cancel', place_path(@place), class: 'btn btn-secondary btn-sm' %>
</h1>
<%= render 'form', trip: @trip, place: @place %>
<%= link_to 'Delete', place_path(@place), method: :delete, class: "btn btn-danger", data: { confirm: "Are you sure you want to delete this Place?\nYou will lose all of the data associated with it." } %>
